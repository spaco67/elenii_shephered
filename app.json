{
  "expo": {
    "name": "Elenii",
    "slug": "elenii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "elenii",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*", "assets/model/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elenii.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elenii.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-camera", "expo-location"],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true
  }
}
